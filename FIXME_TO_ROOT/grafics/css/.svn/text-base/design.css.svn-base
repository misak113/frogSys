    margin-bottom: 0px;
    padding-bottom: 0px;
    margin-top: 3px;
    padding-top: 0px;
    width: 160px;
    float:none;
}

.menu_left_in {

    width: 220px;
}

.menu_left_item a {
    font-family: Arial;
    font-size: 12px;
}

